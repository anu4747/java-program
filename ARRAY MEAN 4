import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        double sum = 0;
        for (int i = 0; i < n; i++) {
            int num = scanner.nextInt();
            sum += num;
        }

        double mean = sum / n;

    
        System.out.printf("Array Mean Value is %.2f\n", mean);

        scanner.close();
    }
}

    
